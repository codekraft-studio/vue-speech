{
  "name": "@codekraft-studio/vue-speech",
  "version": "0.1.0",
  "description": "VueJs integration and components for the Web Speech API",
  "scripts": {
    "prepublishOnly": "npm run build",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name VueSpeech src/components/index.js",
    "lint": "vue-cli-service lint",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs"
  },
  "main": "./dist/VueSpeech.umd.js",
  "files": [
    "dist"
  ],
  "dependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.5",
    "@vue/cli-plugin-eslint": "^4.0.5",
    "@vue/cli-service": "^4.0.5",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.3.1",
    "vuepress": "^1.2.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "publishConfig": {
    "access": "public"
  }
}
